                itemResponse.endTime = responseEndTime.toUTCString();
                itemResponse.status = "MAX_TIMEOUT";
                itemResponse.score = score;
                //itemResponse.scoreInfo = scoreInfo;
                postItemResponse(stream);
                //processItem(stream);
